                @Html.DisplayFor(model => model.DateCommand.Date)
            </dd>

            <dt>
                Seller
            </dt>

            <dd>
                @ViewBag.SellerName
            </dd>

            <dt>
                Client
            </dt>

            <dd>
                @ViewBag.ClientName
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.CommandID }) |
        @Html.ActionLink("Back to List", "Index")
